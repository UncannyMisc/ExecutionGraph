    "cmake.useCMakeServer": true,
    // "C_Cpp.clang_format_fallbackStyle": "none",
    // "C_Cpp.clang_format_style": "file",
    "editor.formatOnSave": false,
    "fileHeaderComment.parameter": {
        "*": {
            "commentbegin": "// ",
            "commentprefix": "// ",
            "commentend": "// ",
            "bar": "=========================================================================================",
            "barComment": "/* ---------------------------------------------------------------------------------------*/",
            "header": [
                " ExecutionGraph",
                " Copyright (C) 2014 by Gabriel Nützi <gnuetzi (at) gmail (døt) com>"
            ],
            "license": [
                " This Source Code Form is subject to the terms of the Mozilla Public",
                " License, v. 2.0. If a copy of the MPL was not distributed with this",
                " file, You can obtain one at http://mozilla.org/MPL/2.0/."
            ]
        }
    },
    "fileHeaderComment.template": {
        "*": [
            "${commentbegin}${bar}",
            "${commentbegin}${header}",
            "${commentbegin}",
            "${commentbegin} @date ${date}",
            "${commentbegin} @author ${authorName}, ${authorEmail}",
            "${commentbegin}",
            "${commentbegin}${license}",
            "${commentbegin}${bar}"
        ],
        "funcDoc" : [
            "//! Description",
            "//! ",
            "//! @return  ReturnValue",
            "//!"
        ],
        "classDoc" : [
            "${barComment}",
            "/*!",
            "    Description",
            "",
            "    @date ${date}",
            "    @author ${authorName}, ${authorEmail}",
            " */",
            "${barComment}",
        ],
        "cpp&tsHeader": [
            "${commentbegin}${bar}",
            "${commentbegin}${header}",
            "${commentbegin}",
            "${commentbegin} @date ${date}",
            "${commentbegin} @author ${authorName}, ${authorEmail}",
            "${commentbegin}",
            "${commentbegin}${license}",
            "${commentbegin}${bar}"
        ]
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/bower_components": true,
        "**/jspm_packages": true,
        "**/node_modules": true,
    },
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/node_modules/**": true,
        "**/build/**" : true,
        "**/buildExternal/**" : true,
        "**/dist/**" : true
    },
    "search.exclude": {
        "**/build/**" : true,
        "**/buildExternal/**" : true,
        "**/dist/**" : true
    },
    "cmake.buildDirectory": "${workspaceRoot}/build",
    "cmake.configureSettings": {
                            "ExecutionGraph_BUILD_TESTS" : true,
                            "ExecutionGraph_BUILD_LIBRARY" : true,
                            "ExecutionGraph_BUILD_GUI" : true,
                            "ExecutionGraph_EXTERNAL_BUILD_DIR" : "${workspaceRoot}/buildExternal",
                            "ExecutionGraph_USE_ADDRESS_SANITIZER" : true,
                            "CMAKE_EXPORT_COMPILE_COMMANDS" : true
    },
    "cmake.configureArgs": ["--parallel"],
    "plantuml.exportFormat": "svg",
    "plantuml.exportSubFolder": false,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "C_Cpp.clang_format_fallbackStyle": "none",
    "C_Cpp.loggingLevel": "Debug",
    "git.ignoreLimitWarning": true,
    "cmake.configureOnOpen": false,
    "C_Cpp.configurationWarnings": "Disabled",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.mode.active": "review",
    "python.formatting.provider": "yapf",
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    
}