{
  "C_Cpp.clang_format_fallbackStyle": "none",
  "C_Cpp.configurationWarnings": "Enabled",
  "C_Cpp.loggingLevel": "Debug",
  "cmake.buildDirectory": "${workspaceRoot}/build",
  "cmake.configureArgs": [
    "--parallel"
  ],
  "cmake.configureOnOpen": false,
  "cmake.configureSettings": {
    "CMAKE_EXPORT_COMPILE_COMMANDS": true,
    "ExecutionGraph_BUILD_GUI": false,
    "ExecutionGraph_BUILD_LIBRARY": true,
    "ExecutionGraph_BUILD_SERIALIZATION": false,
    "ExecutionGraph_BUILD_TESTS": true,
    "ExecutionGraph_EXTERNAL_BUILD_DIR": "${workspaceRoot}/buildExternal",
    "ExecutionGraph_FORCE_MSGLOG_LEVEL": "DEBUG",
    "ExecutionGraph_USE_ADDRESS_SANITIZER": true,
    "CMAKE_VERBOSE_MAKEFILE":true
  },
  "cmake.useCMakeServer": true,
  "editor.formatOnSave": false,
  "fileHeaderComment.parameter": {
    "*": {
      "bar": "=========================================================================================",
      "barComment": "/* ---------------------------------------------------------------------------------------*/",
      "commentbegin": "// ",
      "commentend": "// ",
      "commentprefix": "// ",
      "header": [
        " ExecutionGraph",
        " Copyright (C) 2014 by Gabriel Nützi <gnuetzi (at) gmail (døt) com>"
      ],
      "license": [
        " This Source Code Form is subject to the terms of the Mozilla Public",
        " License, v. 2.0. If a copy of the MPL was not distributed with this",
        " file, You can obtain one at http://mozilla.org/MPL/2.0/."
      ]
    }
  },
  "fileHeaderComment.template": {
    "*": [
      "${commentbegin}${bar}",
      "${commentbegin}${header}",
      "${commentbegin}",
      "${commentbegin} @date ${date}",
      "${commentbegin} @author ${authorName}, ${authorEmail}",
      "${commentbegin}",
      "${commentbegin}${license}",
      "${commentbegin}${bar}"
    ],
    "classDoc": [
      "${barComment}",
      "/*!",
      "    Description",
      "",
      "    @date ${date}",
      "    @author ${authorName}, ${authorEmail}",
      " */",
      "${barComment}"
    ],
    "cpp&tsHeader": [
      "${commentbegin}${bar}",
      "${commentbegin}${header}",
      "${commentbegin}",
      "${commentbegin} @date ${date}",
      "${commentbegin} @author ${authorName}, ${authorEmail}",
      "${commentbegin}",
      "${commentbegin}${license}",
      "${commentbegin}${bar}"
    ],
    "funcDoc": [
      "//! Description",
      "//! ",
      "//! @return  ReturnValue",
      "//!"
    ]
  },
  "files.associations": {
    "*.cpp": "cpp",
    "*.hpp": "cpp",
    "*.icc": "cpp",
    "*.ipp": "cpp",
    "*.tcc": "cpp",
    "__bit_reference": "cpp",
    "__config": "cpp",
    "__debug": "cpp",
    "__errc": "cpp",
    "__functional_03": "cpp",
    "__functional_base": "cpp",
    "__functional_base_03": "cpp",
    "__hash_table": "cpp",
    "__locale": "cpp",
    "__mutex_base": "cpp",
    "__node_handle": "cpp",
    "__nullptr": "cpp",
    "__split_buffer": "cpp",
    "__string": "cpp",
    "__threading_support": "cpp",
    "__tree": "cpp",
    "__tuple": "cpp",
    "algorithm": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "cfenv": "cpp",
    "chrono": "cpp",
    "cinttypes": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "codecvt": "cpp",
    "complex": "cpp",
    "condition_variable": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "exception": "cpp",
    "filesystem": "cpp",
    "forward_list": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "future": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "hashtable": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "ostream": "cpp",
    "queue": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "regex": "cpp",
    "registration": "cpp",
    "set": "cpp",
    "sstream": "cpp",
    "stack": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "strstream": "cpp",
    "system_error": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "type": "cpp",
    "type_traits": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "utility": "cpp",
    "valarray": "cpp",
    "variant": "cpp",
    "vector": "cpp",
    "shared_mutex": "cpp"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/bower_components": true,
    "**/jspm_packages": true,
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/build/**": true,
    "**/buildExternal/**": true,
    "**/client/build/**": true,
    "**/dist/**": true,
    "**/docs/demo/**": true,
    "**/node_modules/**": true
  },
  "git.ignoreLimitWarning": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.enabled": false,
  "gitlens.mode.active": "zen",
  "gitlens.views.fileHistory.enabled": false,
  "gitlens.views.lineHistory.enabled": false,
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "plantuml.exportFormat": "svg",
  "plantuml.exportOutDir": "",
  "plantuml.exportSubFolder": false,
  "python.formatting.provider": "yapf",
  "search.exclude": {
    "**/build/**": true,
    "**/buildExternal/**": true,
    "**/dist/**": true,
    "**/docs/demo/**": true
  },
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "https://www.plantuml.com/plantuml"
}