{
    "configurations": [
        {
            "name": "Mac/Linux",
            "includePath": [
                "${workspaceRoot}/include",
                "${workspaceRoot}/gui",
                "${workspaceRoot}/gui/executionGraphGui/client",
                "${workspaceRoot}/build/include",
                "${workspaceRoot}/tests/include"
            ],
            "defines": [],
            "browse": {
                "path": [
                    "${workspaceRoot}/src",
                    "${workspaceRoot}/include",
                    "${workspaceRoot}/gui",
                    "${workspaceRoot}/build/include",
                    "${workspaceRoot}/tests/include"
