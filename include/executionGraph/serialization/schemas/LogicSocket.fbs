namespace executionGraph.serialization;

table LogicSocket {
    typeIndex:uint64 (id: 0); //!< The type index of this socket in the type list `Config::SocketTypes`.
    type:string (id:1);       //!< The type rtti of this socket.
    typeName:string (id:2);   //!< The readable type name of this socket.
    
    index:uint64 (id:3);      //!< The slot index of this socket.
}